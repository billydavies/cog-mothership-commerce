{% extends '::order:detail:view' %}

{% block order_content %}
	<div class="container-content short">
		{% if not addresses is empty %}
			{% for address in addresses %}
				{% include '::order:detail:address:detail' %}
			{% endfor %}
		{% else %}
			<p> {{ 'ms.commerce.order.address.none'|trans }}</p>
		{% endif %}
	</div>
{% endblock %}