<a href="{{ url('ms.commerce.product.create')}}" class="create" data-live>Add product</a>

<ul class="main-menu">
{% for category, products in listing %}
	<li>{{ category }}
		<ul>
		{% for product in products %}
			<li><a href="{{ url('ms.commerce.product.edit', {productID:product.id}) }}" data-live>{{ product.name }}</a></li>
		{% endfor %}
		</ul>
	</li>
{% endfor %}
</ul>